<script>
    function CloseMenu(){
        let menu = document.getElementById("sidebar-wrapper")
        if (menu) {
            let displayValue = window.getComputedStyle(menu).getPropertyValue("display");
            if (displayValue != 'none')
            {
                menu.style.display = "none";
            }
            else menu.style.display = "block";
        }
    }
</script>



