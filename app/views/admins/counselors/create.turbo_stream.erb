<% if @counselor.type == 'User' %>
  <%= turbo_stream.prepend "users-tbody", partial: "admins/users/partials/user", locals: { user: @counselor } %>
<% else %>
  <%= turbo_stream.prepend "counselors-tbody", partial: "admins/counselors/partials/counselor", locals: { counselor: @counselor } %>
<% end %>
<%= turbo_stream.update "modal_for_counselor", "Success created" %>
