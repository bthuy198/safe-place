<% if @counselor.type == 'Counselor' %>
  <%= turbo_stream.replace "counselor-#{@counselor.id}", partial: "admins/counselors/partials/counselor", locals: { counselor: @counselor } %>
<% else %>
  <%= turbo_stream.remove "counselor-#{@counselor.id}" %>
<% end %>
<%= turbo_stream.update "modal_for_counselor", "Success Edit" %>
